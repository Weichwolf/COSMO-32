10 REM Array (DIM) test
20 DIM SQUARES(10)
30 DIM NAMES$(5)
40 REM Fill integer array
50 FOR INDEX = 0 TO 9
60 LET SQUARES(INDEX) = INDEX * INDEX
70 NEXT INDEX
80 PRINT "Integer array SQUARES(i) = i*i:"
90 FOR INDEX = 0 TO 9
100 PRINT "SQUARES("; INDEX; ") = "; SQUARES(INDEX)
110 NEXT INDEX
120 REM Fill string array
130 NAMES$(0) = "ZERO"
140 NAMES$(1) = "ONE"
150 NAMES$(2) = "TWO"
160 NAMES$(3) = "THREE"
170 NAMES$(4) = "FOUR"
180 PRINT "String array:"
190 FOR INDEX = 0 TO 4
200 PRINT "NAMES$("; INDEX; ") = "; NAMES$(INDEX)
210 NEXT INDEX
220 PRINT "Test complete!"
230 END
